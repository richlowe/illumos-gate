BEGIN {
	print "#"
	print "# This file is autogenerated by dtrace.mapfile.awk"
	print "#"
	print "$mapfile_version 2"
	print "SYMBOL_SCOPE {"
	print "    global:"
}

/__dtrace_probe_/ {
	printf "\t%s\t{ FLAGS = EXTERN };\n", $1
}

END {
	print "};"
}
