include ../../Makefile.master

MAN3PLOTFILES = plot.3plot

MAN3PLOTSOFILES =	arc.3plot	\
		 	box.3plot	\
		 	circle.3plot	\
		 	closepl.3plot	\
		 	closevt.3plot	\
		 	cont.3plot	\
		 	erase.3plot	\
		 	label.3plot	\
		 	line.3plot	\
