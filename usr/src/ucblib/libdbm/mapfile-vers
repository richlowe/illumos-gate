$mapfile_version 2

SYMBOL_VERSION SUNW_1.1 {
    global:
	bitno		{ ASSERT = { TYPE = OBJECT; SIZE = addrsize; }; };
	blkno		{ ASSERT = { TYPE = OBJECT; SIZE = addrsize; }; };
	dbmclose;
	dbminit;
	dbrdonly	{ ASSERT = { TYPE = OBJECT; SIZE = 4; }; };
	delete;
	dirbuf		{ ASSERT = { TYPE = OBJECT; SIZE = 4096; }; };
	dirf		{ ASSERT = { TYPE = OBJECT; SIZE = 4; }; };
	fetch;
	firstkey;
	hmask		{ ASSERT = { TYPE = OBJECT; SIZE = addrsize; }; };
	maxbno		{ ASSERT = { TYPE = OBJECT; SIZE = addrsize; }; };
	nextkey;
	pagbuf		{ ASSERT = { TYPE = OBJECT; SIZE = 1024; }; };
	pagf		{ ASSERT = { TYPE = OBJECT; SIZE = 4; }; };
	store;
};

SYMBOL_VERSION SUNWprivate_1.1 {
    global:
	calchash;
	hashinc;
	makdatum;
    local:
	*;
};
